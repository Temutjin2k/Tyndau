.PHONY: initClean

initClean:
	cmd /c mkdir $(DIR)\cmd
	cmd /c mkdir $(DIR)\config
	cmd /c mkdir $(DIR)\internal
	cmd /c mkdir $(DIR)\internal\adapter
	cmd /c mkdir $(DIR)\internal\adapter\grpc
	cmd /c mkdir $(DIR)\internal\adapter\grpc\server
	cmd /c mkdir $(DIR)\internal\adapter\grpc\server\frontend
	cmd /c mkdir $(DIR)\internal\adapter\grpc\server\frontend\dto
	cmd /c mkdir $(DIR)\internal\adapter\postgres
	cmd /c mkdir $(DIR)\internal\adapter\postgres\dao
	cmd /c mkdir $(DIR)\internal\app
	cmd /c mkdir $(DIR)\internal\model
	cmd /c mkdir $(DIR)\internal\usecase
	cmd /c mkdir $(DIR)\migrations
	cmd /c mkdir $(DIR)\pkg
